{
  "name": "crypto-portfolio-tracker",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "nest build api",
    "build:clean": "npm run clean && nest build api -b webpack",
    "clean": "rimraf dist && rimraf libs/**/node_modules/.cache",
    "start": "nest start",
    "start:dev": "nest start api --watch",
    "start:prod": "node dist/apps/api/main",
    "frontend:dev": "npm run dev -w apps/frontend",
    "frontend:build": "npm run build -w apps/frontend",
    "dev": "concurrently \"npm run start:dev\" \"npm run frontend:dev\"",
    "build:all": "npm run build && npm run frontend:build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.3.0",
    "@nestjs/websockets": "^10.4.15",
    "@prisma/client": "^6.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/joi": "^17.2.2",
    "@types/passport-jwt": "^4.0.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "ccxt": "^4.4.46",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cls-hooked": "^4.2.2",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "rate-limiter-flexible": "^3.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.3.0",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.6.1",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.0",
    "@types/cls-hooked": "^4.3.9",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "@types/webpack-node-externals": "^3.0.4",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "cypress": "^13.13.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
