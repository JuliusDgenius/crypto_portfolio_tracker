{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./dist/tsconfig.tsbuildinfo",
    "module": "commonjs",
    "declaration": false,
    "removeComments": true,
    "incremental": true,
    "baseUrl": "./",
    "paths": {
      "@libs/libs/watchlist": [
        "libs/libs/watchlist/src"
      ],
      "@libs/libs/watchlist/*": [
        "libs/libs/watchlist/src/*"
      ],
      "@libs/watchlist": [
        "libs/watchlist/src"
      ],
      "@libs/watchlist/*": [
        "libs/watchlist/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}